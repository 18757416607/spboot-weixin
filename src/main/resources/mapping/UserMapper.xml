<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="UserMapper">
	
	<!--表名 -->
	<sql id="tableName">
		"tb_user"
	</sql>
	
	<!-- 字段 -->
	<sql id="Field">
		"id","name","date"
	</sql>
	
	<!-- 字段值 -->
	<sql id="FieldValue">
			#{id},#{name},#{date}
	</sql>
	
	<select id="findUserAll">
		SELECT  * FROM tb_user
	</select>
</mapper>